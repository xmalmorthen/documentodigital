<link href="@Url.Content("~/Content/Detalle_Usuario.css")" rel="stylesheet" type="text/css" />

@model TramiteDigitalWeb.data_members.ca_nodos

@{
    ViewBag.Title = "Detalle del nodo";
}

<h2>@ViewBag.Title</h2>
<hr style="width: 70%;float: left;display: block;"/>

<fieldset>
    <legend>Nodo</legend>
    <div>
        <div class="display-label">
             @Html.DisplayNameFor(model => model.nodo)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.nodo)
        </div>
    </div>
    <div>
        <div class="display-label">
             @Html.DisplayNameFor(model => model.url_servicio_rest)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.url_servicio_rest)
        </div>
    </div>
    <div>
        <div class="display-label">
             @Html.DisplayNameFor(model => model.usuario)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.usuario)
        </div>
    </div>
    <div>
        <div class="display-label">
             @Html.DisplayNameFor(model => model.activo)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.activo)
        </div>
    </div>
    <div>
        <div class="display-label">
             @Html.DisplayNameFor(model => model.fecha_registro)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.fecha_registro)
        </div>
    </div>
</fieldset>
<p>
    @Html.ActionLink("Editar nodo", "Edita_Nodo", new { id_nodo = Model.id, returnUrl = Request.Url.PathAndQuery }, new { style = "float:left;text-decoration: none;margin-top:5px;background-color: #D3DCE0;border: 1px solid #787878;cursor: pointer;font-size: 1.2em;font-weight: 600;padding: 6px;margin-right: 8px;width: auto;" })
    @Html.ActionLink("Regresar a lista de nodos", "Nodos", null, new { style = "float:left;text-decoration: none;margin-top:5px;background-color: lightcoral;border: 1px solid #787878;cursor: pointer;font-size: 1.2em;font-weight: 600;padding: 6px;margin-right: 8px;width: auto;" })
</p>