@model List<TramiteDigitalWeb.Models.ModulodeAcceso>
@{
    ViewBag.Title = "Menú principal";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                Consultas a los tramites digitales.
            </p>
        </div>
    </section>
}
<div style="text-align:center;margin:50px 0">
    @{
        if (Model.Any(query => query.Modulo.ToString().ToLower() == "consulta local web")){
            @Html.ActionLink(" ", "Consulta", "Consultas", new
            {
                style = "background: url('../../Content/images/buquedalocal.png') no-repeat center right;display: inline-block;height:127px; width:353px;margin: 0 20px;"
            });
        }
        if (Model.Any(query => query.Modulo.ToString().ToLower() == "consulta global web"))
        {
           @Html.ActionLink(" ", "Consulta", "Consultas", new
           {
               style = "background: url('../../Content/images/buquedaglobal.png') no-repeat center right;display: inline-block;height:127px; width:353px;margin: 0 20px;"
           });
        }   
    }
</div>

